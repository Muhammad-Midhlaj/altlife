{
    "source": {
        "include": ["./resources/orp/server"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "documentation/",
        "recurse": true,
        "verbose": true
    }
}
